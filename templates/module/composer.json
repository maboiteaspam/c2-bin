{
  "prefer-stable": true,
  "minimum-stability": "dev",
  "name": "<%= NS.toLowerCase().replace(/\\/g, '/') %>",
  "require": {
    "c/foundation": "dev-master",
    "c/bootstrap": "dev-master"
<% if (modType && modType.match(/design/)) { %>
,"c/welcome": "dev-master"
<% } %>
  },
  "autoload": {
    "psr-4": {
      "<%= NS.replace(/\\/g, "\\\\") %>\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "<%= urlToFoundation.match(/git|http|svn/)?'vcs':'path' %>",
      "url": "<%= urlToFoundation.replace(/\\/g, "\\\\") %>"
    }
    ,{
    "type": "<%= urlToBootstrap.match(/git|http|svn/)?'vcs':'path' %>",
      "url": "<%= urlToBootstrap.replace(/\\/g, "\\\\") %>"
    }
    <% if (modType && modType.match(/design/)) { %>
    ,{
    "type": "<%= urlToWelcome.match(/git|http|svn/)?'vcs':'path' %>",
      "url": "<%= urlToWelcome.replace(/\\/g, "\\\\") %>"
    }
    <% } %>
  ]
}
